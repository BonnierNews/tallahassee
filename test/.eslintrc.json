{
  "extends": "../.eslintrc.json",
  "env": {
    "mocha": true
  },
  "rules": {
    "no-new": 0,
    "no-unused-expressions": 0
  },
  "globals": {
    "Feature": false,
    "Scenario": false,
    "Given": false,
    "When": false,
    "Then": false,
    "And": false,
    "But": false,
    "XPathResult": false,
    "expect": false
  }
}
