{
  "name": "@expressen/tallahassee",
  "version": "9.5.0",
  "description": "Expressen client testing framework",
  "main": "index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" mocha",
    "posttest": "eslint . --cache",
    "test-md": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" node ./scripts/test-md ./README.md && NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" node ./scripts/test-md ./API.md",
    "toc": "node ./scripts/generate-api-toc"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "keywords": [
    "headless",
    "browser",
    "fake",
    "mock",
    "IntersectionObserver",
    "MutationObserver",
    "iframe"
  ],
  "author": "AB Kv√§llstidningen Expressen",
  "repository": {
    "type": "git",
    "url": "git://github.com/ExpressenAB/tallahassee.git"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "domexception": "^1.0.1",
    "node-fetch": "^2.3.0",
    "request": "^2.88.0",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "@bonniernews/wichita": "^0.4.0",
    "chai": "^4.2.0",
    "cookiejar": "^2.1.2",
    "eslint": "^5.15.1",
    "express": "^4.16.4",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.6"
  },
  "files": [
    "lib/",
    "index.js",
    "LICENCE"
  ],
  "bugs": {
    "url": "https://github.com/ExpressenAB/tallahassee/issues"
  },
  "homepage": "https://github.com/ExpressenAB/tallahassee#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
